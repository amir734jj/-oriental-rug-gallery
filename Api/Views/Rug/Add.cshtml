@model Models.Models.Products.Rug

@{
    ViewBag.Title = "Rugs add";
    Layout = "_Layout";
}

<div class="container">
    <h2> Add a new Rug to Inventory </h2>
    
    @using (Html.BeginForm("AddHandler", "Rug", FormMethod.Post))
    {
        @Html.Partial("RugContent", Model, new ViewDataDictionary(ViewData) {{"Type", "Save"}})

        <input class="btn btn-primary" type="submit" value="Add"/>
    }
</div>